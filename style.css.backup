body	{
	background-image:URL('bkground.jpg');
	background-size:100%;
	background-attachment:fixed;
}
.hidden {
	display:none;
}
#content	{
	width:1000px;
	margin:auto;
}
#header img	{
	float:left;
	height:275px;
	margin-left:70px;
}
#header h2 {
	font-size:30pt;
	color:#FF6600;
	clear:left;
	text-align:center;
	/*position:relative;*/
	top:200px;
	font-family:Impact;
	/*margin-left:200px;*/
}
#navigation	{
	height:100%;
	float:left;
}
#navigation a	{ 
	background-color:#687f9d;
	text-decoration: none;
	padding: 6px;
	color: black;
	border-radius: 10px;
	border: solid 3px black;
	margin: 3px; 
	font-size: 1.5em; 
	display: block;
	text-align: center;
	width: 180px;
	font-family:calibri;
}
#navigation a:link	{
	background-color: #FF6600;
}
#navigation a:visited	{
	background-color: #FF6600;
}
#navigation a:hover { 
	border: 3px solid #3270C8;
}
#navigation ul	{
	list-style-type: none;
	margin: 0;
	padding-left: 0;
}
#login,#register	{
	float:left;
    margin: 2px;
	margin-top:10px;
    padding: 7px 18px 8px;
    font-size: 16px;
    background-color: #0A417A;
    border-radius: 4px;
    border: medium none;
    font-weight: normal;
    color: aqua;
    cursor: pointer;
    display: inline-block;
    text-align: center;
    text-decoration: none;
}
#reg	{
	float:right;
	margin-top:50px;
}
#info	{
	margin-left:250px;
}
#topNav	a{
	margin: 2px;
	padding: 10px;
	font-size: 20px;
	background-color: #99DAFC;
	border-radius: 100px;
    border: dotted #FF6600;
	font-weight:bold;
	font-family:calibri;
	text-align: center;
    text-decoration: none;
}
#top	{
	margin-top:25px;
	margin-top:50px;
}
#top	.movie{
	height:100px;
	font-family:calibri;
	font-weight:bold;
	font-size:15pt;
	padding:5px;
	margin-top:3px;
	width:700px;
	margin:auto;
}
#top	.movie	img	{
	margin-right:10px;
	width:60px;
	height:100px;
	float:left;
}
#JoinUsImage img {
	width: 800px;
	height: 170px;
	margin-left:30px;
	margin-bottom: 20px;
}
#top h2	{
	font-family:calibri;
}

#contactUs {
	float:right;
	background-color:grey;
	width:170px;
	height:150px;
}
#contactUs #work {
	width:80px;
	margin-top:10px;
	margin-left:30px;
	position:center;
	text-align:center;
	text-decoration:none;
	padding: 7px 18px 8px;
    font-size: 16px;
    background-color: #0A417A;
    border-radius: 4px;
    border: medium none;
    font-weight: normal;
    color: aqua;
    cursor: pointer;
    display: inline-block;
}

#about {
	margin-left:230px;
}

#vacancy h2 {
	margin-left:230px;
	text-decoration:underline;
	font-size:25pt;
}
#vacancy dl {
	margin-left:230px;
}
#vacancy dt {
	color:red;
	font-size:20pt;
	font-weight:bold;
}
#vacancy dd {
	font-size:15pt;
	font-weight:bold;
	margin-bottom:10px;
}

#movie #img {
	float:left;
	margin-left:20px;
}
#movie #mSpecs {
	float:right;
	margin-right:200px;
	width:300px;
	text-align:justify;
}
#meter	{
	width:170px;
}
#buy {
	width:100px;
	font-weight:bold;
	text-decoration:none;
	display:block;
    border: 0px none;
    padding: 10px;
    margin: 0px;
    background-color: #A52A2A;
	border-radius:30px;
    cursor: pointer;
    vertical-align: middle;
    color: white;
    text-align: center;
}

#buyForm  label {
	display:block;
	margin-top:20px;
	margin-bottom:20px;
	margin-left:230px;
	font-weight:bold;
	color:blue;
}